{
  "name": "@freephoenix888/generate-usage-ways-of-npm-cli-apps-in-markdown-format",
  "version": "4.1.2",
  "description": "Generates usage ways of CLI applications of npm package in markdown format",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "bin": {
    "generate-usage-ways-of-npm-cli-apps-in-markdown-format": "dist/cli/generate-usage-ways-of-npm-cli-apps-in-markdown-format.js"
  },
  "repository": {
    "url": "https://github.com/FreePhoenix888/generate-usage-ways-of-npm-cli-apps-in-markdown-format"
  },
  "scripts": {
    "library:build:clean": "fse remove dist && npm run library:build:dirty",
    "library:build:dirty": "ts-node --esm ./automation/library/build.ts",
    "library:build:generate-package-class": "generate-package-class",
    "documentation:generate": "cross-env DEBUG=* ts-node --esm ./automation/documentation/generate.ts",
    "library:documentation:generate": "typedoc ./src/main.ts"
  },
  "keywords": [
    "cli-usage",
    "automation",
    "generation",
    "markdown-generation",
    "cli",
    "help",
    "automation",
    "generate",
    "generation",
    "markdown",
    "cli-app",
    "cli-application",
    "console-app",
    "console-application",
    "readme",
    "readme-generation",
    "markdown-generation",
    "documentation",
    "documentation-generation",
    "doc-generation",
    "npm-cli-app",
    "npm-cli-applaction",
    "npm-console-application"
  ],
  "author": "FreePhoenix888",
  "license": "Unlicense",
  "files": [
    "dist"
  ],
  "dependencies": {
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.7",
    "@deep-foundation/npm-automation": "^16.0.0",
    "@types/debug": "^4.1.8",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.2",
    "@types/yargs": "^17.0.24",
    "glob": "^10.3.3",
    "ts-node": "^10.9.1",
    "types-package-json": "^2.0.39",
    "typescript": "^5.1.6"
  }
}
